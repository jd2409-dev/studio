
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Allow read/write access to all paths for any authenticated user
    // WARNING: This is insecure and should NOT be used in production.
    // Replace with specific rules (e.g., based on user ID) before deploying.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
